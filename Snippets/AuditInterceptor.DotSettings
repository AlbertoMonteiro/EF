	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=B18FCBA5ECD0BE42BBB0B259AE8369A3/Text/@EntryValue">public static void DropAllForeignKeys(this DbMigration migration, string tableWithFk, string columnFk, string tableReferenced)&#xD;
